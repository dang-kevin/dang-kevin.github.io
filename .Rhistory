#Normal QQ
crab %>%
mutate_at("size",(. - mean(.)) / sd(.)) %>%
arrange(size) %>%
mutate(q = qnorm(1:n() / (n() + 1))) %>%
ggplot(aes(x = q,y = size)) +
theme_classic() +
geom_point(pch=21) +
geom_abline(slope = 1,intercept = 0,colour = "red") +
labs(title = "Normal QQ-plot",
subtitle = "Crab Data",
x = "Theoretical Quantiles",
y = "Sample Quantiles")
